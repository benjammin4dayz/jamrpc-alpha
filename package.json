{
  "name": "neu-node",
  "version": "1.0.0",
  "description": "Neutralino with a Node.js backend",
  "main": "resources/index.html",
  "scripts": {
    "build": "neu build --release",
    "clean": "rm -rf bin/ dist/ node_modules/ extensions/node/node_modules",
    "fetch:deps": "neu update && cd extensions/node && npm install",
    "prep": "npm run fetch:deps && npm run neu:trim-bin",
    "prep:dev": "npm install && npm run prep",
    "neu:trim-bin": "rm bin/neutralino-mac* bin/neutralino-linux_arm*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "benjammin4dayz",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0"
  }
}
